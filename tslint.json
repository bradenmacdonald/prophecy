{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "max-classes-per-file": [false],
        "member-access": [false],
        "no-bitwise": false,
        "quotemark": [false],
        "max-line-length": [
            true, 150
        ],
        "member-ordering": [false]
    },
    "rulesDirectory": []
}
